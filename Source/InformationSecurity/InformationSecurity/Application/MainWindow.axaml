<Window 
    xmlns="https://github.com/avaloniaui"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
    
    xmlns:app="using:InformationSecurity.Application"
    xmlns:pc="using:InformationSecurity.Pages.Cryptography"
    
    x:CompileBindings="True"
    x:Class="InformationSecurity.Application.MainWindow"
    x:DataType="app:MainViewModel"
    
    Icon="/Shared/Assets/avalonia-logo.ico"
    Title="InformationSecurity">
    
    <pc:CryptographyPage DataContext="{Binding CryptographyPage}"/>
    
</Window>

