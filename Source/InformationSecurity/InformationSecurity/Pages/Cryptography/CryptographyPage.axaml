<UserControl 
	xmlns="https://github.com/avaloniaui"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
	
	xmlns:pc="using:InformationSecurity.Pages.Cryptography"
	xmlns:wc="using:InformationSecurity.Widgets.Cryptography"
	xmlns:sp="using:InformationSecurity.Shared.Pagination"
	xmlns:pagination="clr-namespace:InformationSecurity.Widgets.Pagination"

	x:Class="InformationSecurity.Pages.Cryptography.CryptographyPage"
	x:DataType="pc:CryptographyPageModel"
	x:CompileBindings="True"
	Background="#2a2a2a">

	<Grid RowDefinitions="*,Auto,*" 
	      ColumnDefinitions="*,Auto,*">
		<pagination:PaginationWidget 
			DataContext="{Binding Pages}"
			Grid.Row="1" 
			Grid.Column="1"/>
	</Grid>
</UserControl>
